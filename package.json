{
    "name": "vecto-backend",
    "version": "1.0.0",
    "description": "Backend API for Vecto application using Fastify and Sequelize",
    "main": "src/server.js",
    "scripts": {
        "start": "node src/server.js",
        "dev": "nodemon src/server.js",
        "migrate": "sequelize-cli db:migrate",
        "migrate:undo": "sequelize-cli db:migrate:undo",
        "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
        "seed": "sequelize-cli db:seed:all",
        "seed:undo": "sequelize-cli db:seed:undo:all"
    },
    "keywords": [
        "fastify",
        "sequelize",
        "postgresql",
        "mvc",
        "api"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@fastify/cors": "^10.0.1",
        "@fastify/env": "^5.0.1",
        "@fastify/jwt": "^10.0.0",
        "@fastify/rate-limit": "^10.3.0",
        "@fastify/swagger": "^9.6.1",
        "@fastify/swagger-ui": "^5.2.4",
        "bcrypt": "^6.0.0",
        "dotenv": "^16.4.7",
        "fastify": "^5.2.0",
        "fastify-plugin": "^5.1.0",
        "pg": "^8.13.1",
        "pg-hstore": "^2.3.4",
        "pino": "^9.5.0",
        "sequelize": "^6.37.5"
    },
    "devDependencies": {
        "jest": "^30.2.0",
        "nodemon": "^3.1.9",
        "pino-pretty": "^13.0.0",
        "sequelize-cli": "^6.6.2",
        "supertest": "^7.2.2"
    }
}
